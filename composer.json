{
    "name": "monkeypod/api",
    "description": "PHP SDK for interacting with the MonkeyPod API",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Huttler",
            "email": "adam@monkeypod.io"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/events": ">=9",
        "illuminate/http": ">=9",
        "guzzlehttp/guzzle": "^7.4",
        "phpdocumentor/reflection-docblock": "^5.3"
    },
    "require-dev": {
        "illuminate/testing": ">=8",
        "phpunit/phpunit": "^9.5",
        "ramsey/uuid": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "MonkeyPod\\Api\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MonkeyPod\\Api\\Tests\\": "tests"
        }
    }
}
